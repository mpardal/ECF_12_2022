{% extends 'base.html.twig' %}

{% block title %}Modification d'un nouveau partenaire{% endblock %}

{% block body %}
    <main class="container">
        <h2 class="text-center m-4">Informations de la franchise</h2>
        {{ form_start(franchiseRegistrationForm) }}
        {% include "franchise/franchise_form.html.twig" %}
        <div class="d-flex flex-row justify-content-around m-3">
            <a href="{{ path('app_admin_list_franchise') }}" class="rounded-3 p-2 btn btn-danger">
                    Annuler
            </a>
            <button class="rounded-3 p-2 btn btn-success">
                Valider
            </button>
        </div>
        {{ form_end(franchiseRegistrationForm) }}
    </main>
{% endblock %}